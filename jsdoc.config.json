{
  "source": {
    "include": ["src", "jsdoc-types.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": ["jsdoc-export-default-interop"],
  "templates": {
    "default": { "outputSourceFiles": false, "includeDate": false }
  }
}
