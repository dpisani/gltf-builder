{
  "name": "gltf-builder",
  "version": "0.4.1",
  "description": "A builder for the GLTF 3D transmission format",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "./dist/types.d.ts",
  "author": "Dean Pisani",
  "license": "MIT",
  "private": false,
  "repository": "github:dpisani/gltf-builder",
  "homepage": "https://github.com/dpisani/gltf-builder",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "NODE_ENV=test mocha --require babel-register \"src/**/*test.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "docs": "rm -rf docs && jsdoc -c jsdoc.config.json -r -d docs",
    "build:types": "jsdoc -c jsdoc.config.json -t node_modules/tsd-jsdoc/dist -r -d dist"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "gltf-validator": "^2.0.0-dev.2.5",
    "jsdoc": "^3.5.5",
    "jsdoc-export-default-interop": "^0.3.1",
    "mocha": "^4.0.1",
    "prettier": "^1.14.2",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.0.0",
    "should": "^13.2.0",
    "should-sinon": "^0.0.6",
    "sinon": "^4.2.2"
  },
  "dependencies": {
    "base64-js": "^1.3.0",
    "lodash": "^4.17.4",
    "tsd-jsdoc": "^2.1.3"
  }
}
