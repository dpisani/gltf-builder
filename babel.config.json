{
  "env": {
    "main": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "10"
            }
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-object-rest-spread"],
      "ignore": ["**/*test.js", "**/test-util"]
    },
    "module": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "10"
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-object-rest-spread"],
      "ignore": ["**/*test.js", "**/test-util"]
    },
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "10"
            }
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-object-rest-spread"]
    }
  }
}
